  "keywords": [
    "api",
    "rest",
    "pubsub",
    "restful",
    "iot",
    "internet of things",
    "devices",
    "cloud",
    "realtime",
    "connected",
    "mqtt",
    "socketio"
  ],
  "dependencies": {
    "extend": "3.0.2",
    "request": "2.88.0",
    "socket.io-client": "2.3.0",
    "@hapi/joi": "16.1.8",
    "mqtt": "2.18.8"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "jshint": "2.10.3",
    "mocha": "6.2.2"
